<ViewObject
   Name="ProjectLOVVO"
   TimeStamp="1019576051145"
   OrderBy="projectnumber"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="oracle.apps.hxc.selfservice.timecard.server.ProjectLOVVOImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select  
    project_number projectnumber, 
    project_name, 
    project_id projectid,  
    pa_online_projects_v2.start_date, 
    completion_date, 
    carrying_out_organization_id, 
    taskid, 
    'Labour Force' expenditureType, 
    'ST' SystemLinkageFunction 
from    pa_online_projects_v2 
INNER JOIN (select EMPLOYEE_NUMBER 
            from per_all_people_f 
            where person_id = fnd_global.employee_id 
            and sysdate between EFFECTIVE_START_DATE and EFFECTIVE_END_DATE) pers  
    ON pa_online_projects_v2.EMPLOYEE_NUMBER = pers.EMPLOYEE_NUMBER
   ]]></SQLQuery>
