   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT SecGuidelinesTLEO.TITLE AS TITLE, 
       SecGuidelinesTLEO.DESCRIPTION, 
       SecGuidelinesEO.CRITICAL_LEVEL AS CRITICAL_LEVEL, 
       SecGuidelinesStatusEO.STATUS AS STATUS, 
       SecGuidelinesStatusEO.DETAILS, 
       SecGuidelinesEO.AUTOFIXABLE AS AUTOFIXABLE,
       SecGuidelinesTLEO.ROWID AS ROW_ID, 
       SecGuidelinesStatusEO.ROWID AS ROW_ID1,        	
decode(SecGuidelinesStatusEO.STATUS, 'P', 'okstatus', 'F', 'failedstatus', 'ER', 'error' , 'SP', 'supress', 'uncompile') AS STATUS_SWITCHER,
decode(SecGuidelinesEO.AUTOFIXABLE, 'Y', 'autofix', 'N', 'manual') AS AUTOFIX_SWITCHER,
SecGuidelinesEO.CODE,
SecGuidelinesEO.ROWID AS ROW_ID2        
FROM FND_SEC_GUIDELINES SecGuidelinesEO, FND_SEC_GUIDELINES_TL SecGuidelinesTLEO, FND_SEC_GUIDELINES_STATUS SecGuidelinesStatusEO
WHERE (SecGuidelinesEO.CODE = SecGuidelinesTLEO.CODE) AND (SecGuidelinesEO.CODE = SecGuidelinesStatusEO.CODE(+)) AND (SecGuidelinesTLEO.LANGUAGE = userenv('LANG'))
   ]]></SQLQuery>
