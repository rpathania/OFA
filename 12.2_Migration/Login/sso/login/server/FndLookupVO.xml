<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!-- $Header: FndLookupVO.xml 120.0 2006/01/20 16:50:14 ssallaka noship $ -->
<ViewObject
   Name="FndLookupVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="oracle.apps.fnd.sso.login.server.FndLookupVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select  LOOKUP_CODE,
        MEANING
  from  FND_LOOKUPS
  where LOOKUP_TYPE = :1
    and ENABLED_FLAG = 'Y'
    and sysdate >= nvl(START_DATE_ACTIVE,sysdate)
    and sysdate <= nvl(END_DATE_ACTIVE,sysdate)
   ]]></SQLQuery>
   <DesignTime>
      <Attr Name="_version" Value="9.0.3.13.88" />
      <Attr Name="_CodeGenFlagNew" Value="36" />
   </DesignTime>
   <ViewAttribute
      Name="LookupCode"
      IsPersistent="false"
      IsNotNull="true"
      Precision="30"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="LOOKUP_CODE"
      Expression="LOOKUP_CODE"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="30" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="Meaning"
      IsPersistent="false"
      IsNotNull="true"
      Precision="80"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="MEANING"
      Expression="MEANING"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="80" />
      </DesignTime>
   </ViewAttribute>
</ViewObject>
